import static org.junit.Assert.*;

import org.junit.Test;

public class TestStringUtilities {
	StringUtilities su = new StringUtilities();
	@Test
	public void testDoesItContainCertainWord() {
		assertTrue(su.DoesItContainCertainWord(" Drop ", "DROP TABLE WHERE"));
	}
	
	@Test
	public void testReplaceDoubleQuotes(){
		assertEquals(su.ReplaceDoubleQuotes("HELLO\"\""), "HELLO\"\"\"\"");
	}
	
	@Test
	public void testReplaceSingleQuotes(){
		assertEquals(su.ReplaceSingleQuotes("HELLO''"), "HELLO''''");
	}
	
	

}
