import static org.junit.Assert.*;

import org.junit.Test;

public class TestStringUtilities {
	StringUtilities su = new StringUtilities();
	@Test
	public void testDoesItContainCertainWord() {
		System.out.println(su.DoesItContainCertainWord("Drop   Table", "DROP TABLE WHERE"));
		assertTrue(su.DoesItContainCertainWord("Drop   Table", "DROP TABLE WHERE"));
	}

}
